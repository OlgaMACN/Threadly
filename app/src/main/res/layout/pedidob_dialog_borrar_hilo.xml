<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"

    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/fondoPantallas">

    <TextView
        android:id="@+id/txtVw_cabeceraGrafico_dialog_deleteHilo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        android:layout_width="match_parent"
        android:layout_height="65dp"
        android:background="@drawable/reutilizable_cabecera"
        android:fontFamily="@font/catchy_mager"
        android:gravity="center"
        android:text="@string/cabeceraGrafico_dialog_deleteHilo"
        android:textAlignment="center"
        android:textColor="@color/textoCabecera_y_Botones"
        android:textSize="25dp" />

    <TextView
        android:id="@+id/txtVw_textoInfo_dialog_deleteHilo"
        android:layout_width="277dp"
        android:layout_height="85dp"
        android:fontFamily="@font/catchy_mager"
        android:gravity="center"
        android:text="@string/textoInfo_dialog_deleteHilo"
        android:textAlignment="center"
        android:textSize="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/txtVw_cabeceraGrafico_dialog_deleteHilo"
        app:layout_constraintVertical_bias="0.100000024" />
    <TextView
        android:id="@+id/txtVw_textoInfo_secundario_dialog_deleteHilo"
        android:layout_width="277dp"
        android:layout_height="85dp"
        android:fontFamily="@font/catchy_mager"
        android:gravity="center"
        android:text="@string/textoInfo_secundario_dialog_deleteHilo"
        android:textAlignment="center"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/txtVw_textoInfo_dialog_deleteHilo"
        app:layout_constraintVertical_bias="0.02" />

    <Button
        android:id="@+id/btn_volver_dialog_pedidob_deleteHilo"
        android:layout_width="114dp"
        android:layout_height="44dp"
        android:backgroundTint="@color/fondoBoton"
        android:fontFamily="@font/catchy_mager"
        android:text="@string/volver_grafico"
        android:textColor="@color/textoCabecera_y_Botones"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.28"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/txtVw_textoInfo_secundario_dialog_deleteHilo"
        app:layout_constraintVertical_bias="0.06" />
    <Button
        android:id="@+id/btn_guardarHilo_dialog_deleteHilo"
        android:layout_width="114dp"
        android:layout_height="44dp"
        android:backgroundTint="@color/fondoBoton"
        android:fontFamily="@font/catchy_mager"
        android:text="@string/guardarHilo_dialog_deleteHilo"
        android:textColor="@color/textoCabecera_y_Botones"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.28"
    app:layout_constraintStart_toEndOf="@id/btn_volver_dialog_pedidob_deleteHilo"
        app:layout_constraintTop_toBottomOf="@id/txtVw_textoInfo_secundario_dialog_deleteHilo"
        app:layout_constraintVertical_bias="0.06" />

</androidx.constraintlayout.widget.ConstraintLayout>